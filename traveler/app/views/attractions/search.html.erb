<h1>Listing attractions</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Id</th>
      <th>Parent</th>
      <th>Categories</th>
      <th>Type</th>
      <th>Title</th>
      <th>Lat</th>
      <th>Long</th>
    </tr>
  </thead>

  <tbody>
    <% @attractions.each do |attraction| %>
      <tr>
        <td><%= attraction["@tag"] %></td>
        <td><%= attraction["@id"] %></td>
        <td><%= attraction["@parent"] %></td>
        <td><%= attraction["@categories"] %></td>
        <td><%= attraction["@type"] %></td>
        <td><%= attraction["@title"] %></td>
        <td><%= attraction["@latitude"] %></td>
        <td><%= attraction["@longitude"] %></td>
       
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>